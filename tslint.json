{
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb"
  ],
  "rules": {
    "no-switch-case-fall-through": false,
    "no-console": [
      true
    ],
    "prefer-for-of": true,
    "prefer-const": true,
    "no-unused-variable": [
      true
    ],
    "quotemark": false,
    "no-console": false,
    "no-var-requires": false,
    "trailing-comma": false,
    "max-classes-per-file": false,
    "function-name": false
  }
}
